{
  "manifest_version": 3,
  "name": "TestMe",
  "version": "1.0.1",
  "icons": {
    "16": "icons/icon16.ico",
    "32": "icons/icon32.ico",
    "48": "icons/icon48.ico",
    "128": "icons/icon128.ico"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["youtube.js"],
      "css": ["popup_styles.css"]
    }
  ]
}